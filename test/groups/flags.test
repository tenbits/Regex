UTest({
	'case(-less)' () {
		[
			'(?i)a(?-i)b(?i)c',
			'(?i)a(?-i:b)c'
		].
		forEach(str  => {
			var rgx = new Regex(str);

			[
				'dAbCd',
				'dabCd',
				'dabcd'
			]
			.forEach(x => {
				rgx.lastIndex = 0;

				var match = rgx.match(x);
				deepEq_(match, {
					value: x.substring(1).slice(0, -1),
					index: 1,
					groups: []
				});
			});

			[
				'ABCd',
				'aBcd',
				'aBbcd',
				'ABcd'
			]
			.forEach(x => {
				var match = rgx.match(x);
				eq_(match, null);
			});
		})
	},
	'should parse regex' () {
		var rgx = new Regex(
		"(?x)\n\t\t\t\t( (https?|s?ftp|ftps|file|smb|afp|nfs|(x-)?man(-page)?|gopher|txmt|issue)://|mailto:)\n\t\t\t\t[-:@a-zA-Z0-9_.,~%+/?=&#;]+(?<![-.,?:#;])\n\t\t\t");

		var match = rgx.match('x-man-page://foo.baz?');
		eq_(match.value, 'x-man-page://foo.baz');
	}
})