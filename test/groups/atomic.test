(function (){
	console.log('Atomic Groups are temporarly disabled, and are transformed into JS non captured');
	return;
	UTest({
		'should match' () {
			Assert({
				regex: 'a(bc|b)c',
				input: 'abcc',
				index: 0,
				match: {
					value: 'abcc',
					index: 0,
					groups: [
						{
							value: 'bc',
							index: 1
						}
					]
				}
			});
			Assert({
				regex: 'a(?>bc|b)c',
				input: 'abcc',
				index: 0,
				match: {
					value: 'abcc',
					index: 0,
					groups: [
						{
							value: 'bc',
							index: 1
						}
					]
				}
			});
		},
		'should once match and fail' () {
			Assert({
				regex: 'a(bc|b)c',
				input: 'abc',
				index: 0,
				match: {
					value: 'abc',
					index: '0',
					groups: [
						{
							value: 'b',
							index: 1
						}
					]
				}
			});
			Assert({
				regex: 'a(?>bc|b)c',
				input: 'abc',
				match: null
			});
		},
	});


	function Assert(data) {
		var rgx = new Regex(data.regex);
		var match = rgx.match(data.input);
		if (data.match == null) {
			eq_(null, match, 'Pattern shoud be matched: ' + data.regex);
			return;
		}

		deepEq_(match, data.match);
	}
}());